<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            file-extensions="pas"
            id="pascalSource"
            name="Pascal Source File"
            priority="high">
      </content-type>
   </extension>
   <extension
         id="FreePascalErrorParser"
         name="Pascaline Error Parser for FreePascal Compiler"
         point="org.eclipse.cdt.core.ErrorParser">
      <errorparser
      		id="es.sidelab.pascaline.core.FreePascalErrorParser"
            class="es.sidelab.pascaline.internal.errorparser.FreePascalErrorParser">
      </errorparser>
   </extension>

</plugin>
