                     id="net.sf.pascaline.managedbuild.fpc.builder.mingw"
                     isAbstract="false"
                     isVariableCaseSensitive="false"
                     superClass="org.eclipse.cdt.build.core.internal.builder">
               </builder>
               <tool
                     id="net.sf.pascaline.managedbuild.tool.fpc.compiler.exe.debug.mingw"
                     superClass="net.sf.pascaline.managedbuild.tool.metafpc.compiler">
               </tool>
            </toolChain>
            </configuration>
            </projectType>
      <projectType
            buildArtefactType="org.eclipse.cdt.build.core.buildArtefactType.exe"
            id="net.sf.pascaline.managedbuild.target.fpc.exe.win.oo"
            isAbstract="false"
            isTest="false"
            name="Executable (FreePascal on Windows)">
         <configuration
               artifactExtension="exe"
               buildProperties="org.eclipse.cdt.build.core.buildType=org.eclipse.cdt.build.core.buildType.debug"
               cleanCommand="rm -rf"
               errorParsers="org.eclipse.cdt.core.MakeErrorParser;net.sf.pascaline.core.FreePascalErrorParser"
               id="net.sf.pascaline.managedbuild.config.fpc.exe.debug.win.oo"
               name="Debug">
            <toolChain
                  archList="all"
                  configurationEnvironmentSupplier="net.sf.pascaline.managedbuilder.fpc.FreePascalEnvironmentVariableSupplier"
                  id="net.sf.pascaline.managedbuild.toolchain.fpc.exe.debug.mingw.oo"
                  isAbstract="false"
                  name="Pascal FreePascal Win32"
                  osList="win32"
                  scannerConfigDiscoveryProfileId="org.eclipse.cdt.managedbuilder.core.GCCManagedMakePerProjectProfile"
                  targetTool="net.sf.pascaline.managedbuild.tool.fpc.compiler.exe.debug.mingw.oo">
               <targetPlatform
		              id="net.sf.pascaline.managedbuild.fpc.targetplatform.mingw.oo"
		              name="Debug Platform"
		              binaryParser="org.eclipse.cdt.core.PE"
		              osList="win32"
		              archList="all">
	              </targetPlatform>
	              <builder
                     id="net.sf.pascaline.managedbuild.fpc.builder.mingw.oo"
                     isAbstract="false"
                     isVariableCaseSensitive="false"
                     superClass="org.eclipse.cdt.build.core.internal.builder">
               </builder>
               <tool
                     id="net.sf.pascaline.managedbuild.tool.fpc.compiler.exe.debug.mingw.oo"
                     superClass="net.sf.pascaline.managedbuild.tool.metafpc.compiler">
               </tool>
            </toolChain>
         </configuration>
